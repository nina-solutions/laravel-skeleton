extends ../../partials/template

block title
  title @lang('press.register')
block content
  h1 @lang('press.register')
  table.table.table-striped
    thead
      tr
        th= 'Campo'
        th= 'Valore'
    tbody
      + foreach('$fields','$field')
        + if('isset($input[$field])')
          tr
            td {{$field}}
            td {{$input[$field]}}